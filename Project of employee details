#include <iostream>
using namespace std;
  
struct employee {
    string ename;
    int age, phn_no;
    int salary;
};
  
// Function to display details of all employees
void display(struct employee emp[], int n)
{
    cout << "Name\tAge\tPhone Number\tSalary\n";
    for (int i = 0; i < n; i++) {
        cout << emp[i].ename << "\t" << emp[i].age << "\t"
             << emp[i].phn_no << "\t" << emp[i].salary << "\n";
    }
}
  
